<?xml version="1.0" encoding="UTF-8"?>


<?import ar.edu.itba.ati.idp.ui.controller.pane.filter.GaussFilterController?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<fx:root type="ar.edu.itba.ati.idp.ui.controller.pane.filter.GaussFilterController"
  alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Label text="Gauss Filter">
      <font>
         <Font name="System Bold" size="13.0" />
      </font>
   </Label>
  <VBox alignment="CENTER" spacing="10.0">
    <Label alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" text="Sigma"/>
    <TextField fx:id="sigmaField" alignment="CENTER" maxHeight="-Infinity"
      maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="100.0"
      promptText="3"/>
  </VBox>
  <Button onAction="#handleApply" fx:id="applyButton" disable="true" mnemonicParsing="false" prefWidth="100.0" text="Apply"/>
</fx:root>
