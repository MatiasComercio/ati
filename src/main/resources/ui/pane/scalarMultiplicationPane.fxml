<?xml version="1.0" encoding="UTF-8"?>

<?import ar.edu.itba.ati.idp.ui.controller.pane.ScalarMultiplicationPaneController?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<fx:root type="ar.edu.itba.ati.idp.ui.controller.pane.ScalarMultiplicationPaneController"
  xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="10.0">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>
  <Label alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
    minWidth="-Infinity" text="Value"/>
  <TextField fx:id="scalarTextField" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
    minHeight="-Infinity" minWidth="-Infinity" prefWidth="100.0" promptText="1.0"/>
  <Button fx:id="applyButton" disable="true" mnemonicParsing="false" prefWidth="100.0"
    text="Multiply" onAction="#handleApply">
    <ar.edu.itba.ati.idp.ui.controller.pane.ScalarMultiplicationPaneController.margin>
      <Insets top="10.0"/>
    </ar.edu.itba.ati.idp.ui.controller.pane.ScalarMultiplicationPaneController.margin>
  </Button>
</fx:root>
